@model VolApp.Models.Vol

<h2>Détails du vol</h2>

<table class="table table-bordered">
    <tr><th>ID</th><td>@Model.Id</td></tr>
    <tr><th>Départ</th><td>@Model.Depart</td></tr>
    <tr><th>Destination</th><td>@Model.Destination</td></tr>
    <tr><th>Date de départ</th><td>@Model.DateDepart</td></tr>
    <tr><th>Date d'arrivée</th><td>@Model.DateArrivee</td></tr>
    <tr><th>Prix</th><td>@Model.Prix MAD</td></tr>
    <tr><th>Places disponibles</th><td>@Model.PlacesDisponibles</td></tr>
</table>

<a asp-action="Dashboard" class="btn btn-secondary">Retour</a>
